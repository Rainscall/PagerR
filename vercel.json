{
    "headers": [
        {
            "source": "/build/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, must-revalidate"
                }
            ]
        },
        {
            "source": "*",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                }
            ]
        }
    ]
}